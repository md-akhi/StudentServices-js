    "dev:nodemon": "cross-env NODE_ENV=development nodemon --exec npm run wb:dev",
    "wb:dev": "run-s webpack:dev babel:dev",
    "gulp:dev": "cross-env NODE_ENV=development gulp",
    "webpack:dev": "webpack",
    "babel:dev": "babel-node src/app.js",
    "build": "npm run prod",
    "prod": "cross-env NODE_ENV=production run-s prod:*",
    "prod:mongod": "sh build/config/Mongod.sh",
    "prod:webpack": "webpack",
    "prod:babel": "babel src -o dist",
    "start:prod": "node dist/app.js",
    "test": "mocha",
    "debug": "set DEBUG=* npm run dev --inspect",
    "prepare": "husky install"
  },
